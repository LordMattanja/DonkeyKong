<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="741.0" prefWidth="545.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.MenuController">
   <children>
      <StackPane prefHeight="150.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <AnchorPane fx:id="backgroundPane" prefHeight="200.0" prefWidth="200.0" />
            <AnchorPane prefHeight="200.0" prefWidth="200.0">
               <children>
                  <VBox alignment="CENTER" prefHeight="741.0" prefWidth="545.0" spacing="80.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                     <children>
                        <Label fx:id="headerLabel" alignment="CENTER" contentDisplay="CENTER" text="Donkey Kong Cloned" textAlignment="CENTER">
                           <font>
                              <Font name="Algerian" size="29.0" />
                           </font>
                        </Label>
                        <VBox alignment="CENTER" prefHeight="60.0" prefWidth="545.0" spacing="10.0">
                           <children>
                              <Label text="Enter your name here:" />
                              <TextField fx:id="playerNameField" maxHeight="-Infinity" maxWidth="-Infinity" prefWidth="150.0" text="Anonymous" />
                           </children>
                        </VBox>
                        <Button fx:id="newGameButton" mnemonicParsing="false" onAction="#startGame" text="New Game" />
                        <Button mnemonicParsing="false" onAction="#showScoreBoard" text="High Score" />
                        <Button fx:id="quitGameButton" mnemonicParsing="false" onAction="#quitGame" text="Quit Game" />
                     </children>
                     <padding>
                        <Insets bottom="100.0" />
                     </padding>
                  </VBox>
               </children>
            </AnchorPane>
         </children>
      </StackPane>
   </children>
</AnchorPane>
